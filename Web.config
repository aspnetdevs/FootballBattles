<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="Game.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>
  <connectionStrings>
    <add connectionString="Data Source=MELNYNETPC\SQLSERVER;Initial Catalog=FootballBattles;Integrated Security=True" name="connectionString"/>
  </connectionStrings>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="AjaxEngineServiceAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
      multipleSiteBindingsEnabled="true" />
    <services>
      <service name="AjaxEngineService">
        <endpoint address="" behaviorConfiguration="AjaxEngineServiceAspNetAjaxBehavior"
          binding="webHttpBinding" contract="AjaxEngineService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>